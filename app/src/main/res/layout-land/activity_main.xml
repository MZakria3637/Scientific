<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="0dp">

    <Button
        android:id="@+id/eight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/eight"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/seven"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/nine"
        app:layout_constraintTop_toBottomOf="@+id/zero"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@drawable/btn_bg"
        android:gravity="end|center_vertical"
        android:text="@string/result"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/x_pow_y"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/powerXy"
        app:layout_constraintBottom_toTopOf="@+id/tanh"
        app:layout_constraintEnd_toStartOf="@+id/cube"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/square"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/square"
        app:layout_constraintBottom_toTopOf="@+id/ex"
        app:layout_constraintEnd_toStartOf="@+id/pi"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/cube"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/cube"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/cube"
        app:layout_constraintBottom_toTopOf="@+id/tan"
        app:layout_constraintEnd_toStartOf="@+id/square"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/x_pow_y"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/tan"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/tan"
        app:layout_constraintBottom_toTopOf="@+id/two"
        app:layout_constraintEnd_toStartOf="@+id/ex"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/tanh"
        app:layout_constraintTop_toBottomOf="@+id/cube"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/two"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/two"
        app:layout_constraintBottom_toTopOf="@+id/equal"
        app:layout_constraintEnd_toStartOf="@+id/one"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/three"
        app:layout_constraintTop_toBottomOf="@+id/tan"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/equal"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/equal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/nine"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/dot"
        app:layout_constraintTop_toBottomOf="@+id/two"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/pi"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/pi"
        app:layout_constraintBottom_toTopOf="@+id/ln"
        app:layout_constraintEnd_toStartOf="@+id/root"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/square"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/root"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/square_root"
        app:layout_constraintBottom_toTopOf="@+id/log"
        app:layout_constraintEnd_toStartOf="@+id/y_underRoot_x"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/pi"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/y_underRoot_x"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/yunderootx"
        app:layout_constraintBottom_toTopOf="@+id/factorial"
        app:layout_constraintEnd_toStartOf="@+id/backspace"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/root"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/backspace"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/backspace"
        app:layout_constraintBottom_toTopOf="@+id/lbracket"
        app:layout_constraintEnd_toStartOf="@+id/CE"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/y_underRoot_x"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/CE"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/CE"
        app:layout_constraintBottom_toTopOf="@+id/rbracket"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/backspace"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/tanh"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/tanh"
        app:layout_constraintBottom_toTopOf="@+id/three"
        app:layout_constraintEnd_toStartOf="@+id/tan"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/x_pow_y"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/ex"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/powerex"
        app:layout_constraintBottom_toTopOf="@+id/one"
        app:layout_constraintEnd_toStartOf="@+id/ln"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/tan"
        app:layout_constraintTop_toBottomOf="@+id/square"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/ln"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/ln"
        app:layout_constraintBottom_toTopOf="@+id/zero"
        app:layout_constraintEnd_toStartOf="@+id/log"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/ex"
        app:layout_constraintTop_toBottomOf="@+id/pi"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/log"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/log"
        app:layout_constraintBottom_toTopOf="@+id/multiply"
        app:layout_constraintEnd_toStartOf="@+id/factorial"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/ln"
        app:layout_constraintTop_toBottomOf="@+id/root"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/factorial"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/factorial"
        app:layout_constraintBottom_toTopOf="@+id/divide"
        app:layout_constraintEnd_toStartOf="@+id/lbracket"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/log"
        app:layout_constraintTop_toBottomOf="@+id/y_underRoot_x"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/lbracket"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/lbracket"
        app:layout_constraintBottom_toTopOf="@+id/plus"
        app:layout_constraintEnd_toStartOf="@+id/rbracket"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/factorial"
        app:layout_constraintTop_toBottomOf="@+id/backspace"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/rbracket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/Rbracket"
        app:layout_constraintBottom_toTopOf="@+id/subtract"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/lbracket"
        app:layout_constraintTop_toBottomOf="@+id/CE"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/three"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/three"
        app:layout_constraintBottom_toTopOf="@+id/dot"
        app:layout_constraintEnd_toStartOf="@+id/two"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tanh"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/one"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/one"
        app:layout_constraintBottom_toTopOf="@+id/nine"
        app:layout_constraintEnd_toStartOf="@+id/zero"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/two"
        app:layout_constraintTop_toBottomOf="@+id/ex"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/zero"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/zero"
        app:layout_constraintBottom_toTopOf="@+id/eight"
        app:layout_constraintEnd_toStartOf="@+id/multiply"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/one"
        app:layout_constraintTop_toBottomOf="@+id/ln"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/multiply"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/multiply"
        app:layout_constraintBottom_toTopOf="@+id/seven"
        app:layout_constraintEnd_toStartOf="@+id/divide"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/zero"
        app:layout_constraintTop_toBottomOf="@+id/log"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/divide"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/divide"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintEnd_toStartOf="@+id/plus"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/multiply"
        app:layout_constraintTop_toBottomOf="@+id/factorial"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/plus"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/plus"
        app:layout_constraintBottom_toTopOf="@+id/five"
        app:layout_constraintEnd_toStartOf="@+id/subtract"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/divide"
        app:layout_constraintTop_toBottomOf="@+id/lbracket"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/subtract"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/subtract"
        app:layout_constraintBottom_toTopOf="@+id/four"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/plus"
        app:layout_constraintTop_toBottomOf="@+id/rbracket"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/dot"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/dot"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/equal"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/three"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/nine"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/nine"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/eight"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/equal"
        app:layout_constraintTop_toBottomOf="@+id/one"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/seven"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/seven"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/six"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/eight"
        app:layout_constraintTop_toBottomOf="@+id/multiply"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/six"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/six"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/five"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/seven"
        app:layout_constraintTop_toBottomOf="@+id/divide"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/five"
       android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/five"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/four"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/six"
        app:layout_constraintTop_toBottomOf="@+id/plus"
        app:layout_constraintVertical_weight="1" />

    <Button
        android:id="@+id/four"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/btn_bg"
        android:text="@string/four"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/five"
        app:layout_constraintTop_toBottomOf="@+id/subtract"
        app:layout_constraintVertical_weight="1" />

</androidx.constraintlayout.widget.ConstraintLayout>